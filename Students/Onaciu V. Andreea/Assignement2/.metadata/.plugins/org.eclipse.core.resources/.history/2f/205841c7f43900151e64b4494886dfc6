
import java.util.Scanner;
//import java.io.PrintWriter;
import java.io.File;
//import java.lang.Integer.*;
 public class InputOutput{
	 
	private Scanner x;
	public void openFile(){
		try{
			x=new Scanner(new File("ser"));
		}
		catch(Exception e){
			System.out.println("Couldn't find this file");
		}
	}
	

	
	public void readCoefficients(){
	
		int[] coefficients=new int[100] ;
		int i=0;
	    String s=x.nextLine();
		String[] coeffs=s.split(" ");
		System.out.println(" coeffs ");
		
		while(coeffs[i]!=null){
			coefficients[i++]=Integer.parseInt(coeffs[i]);
		
			System.out.print(coeffs[i]+" ");
		}	}
		//return coefficients;
	
	
	
	public void readCommands(){
		while (x.hasNextLine()){
		String command=x.nextLine();
	System.out.println(command);//	return command;
		}
		}
    
	
	
	
